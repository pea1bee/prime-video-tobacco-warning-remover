{
  "manifest_version": 3,
  "name": "Prime Video Tobacco Warning Remover",
  "description": "Removes the tobacco warning overlays from Prime Video",
  "version": "1.0",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": ["*://*.primevideo.com/*"],
  "declarative_net_request": {
    "rule_resources": [{
      "id": "ruleset_1",
      "enabled": true,
      "path": "rules.json"
    }]
  }
}